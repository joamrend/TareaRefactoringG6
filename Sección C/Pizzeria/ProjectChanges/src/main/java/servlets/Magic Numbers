PizzaMaker maker = new PizzaMakerData(name, surname, patronymic, hourlyPay);
DAO.getInstance().storePizzaMaker(maker);


class PizzaMakerData {
    String name;
    String surname;
    String patronymic;
    String hourlyPay;

    PizzaMakerData(String n, String s, String p, String pay) {
        name = n;
        surname = s;
        patronymic = p;
        hourlyPay = pay;
    }
}

